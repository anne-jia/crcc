{
  "name": "crcc-mat-manage-ui",
  "version": "1.0.0",
  "description": "物资管理子系统",
  "author": "电采项目组",
  "scripts": {
    "dev": " vue-cli-service serve --mode development ",
    "dev:inIframe": "cross-env VUE_APP_IFRAME_STYLE=in-iframe vue-cli-service serve --mode development",
    "lint": "eslint --ext .js,.vue src",
    "fix": "eslint --fix src",
    "build": "vue-cli-service build",
    "build:dev": "vue-cli-service build --mode development --watch",
    "build:dev:inIframe": "cross-env VUE_APP_IFRAME_STYLE=in-iframe vue-cli-service build --mode development --watch ",
    "build:prod": "vue-cli-service build ",
    "build:inIframe": "cross-env VUE_APP_IFRAME_STYLE=in-iframe vue-cli-service build ",
    "build:stage": "vue-cli-service build --mode staging",
    "build:bundle": "vue-cli-service build --target lib --name @crcc/basic-components src/index.js",
    "publish:crcc": "npm publish --registry=http://172.16.96.117:8081/repository/npm-host/",
    "login:crcc": "npm login --registry=http://172.16.96.117:8081/repository/npm-host/",
    "preview": "node build/index.js --preview",
    "new": "plop",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "test:unit": "jest --clearCache && vue-cli-service test:unit",
    "test:ci": "npm run lint && npm run test:unit"
  },
  "dependencies": {
    "@crcc/bootbase-authorization-ui": "^1.0.53",
